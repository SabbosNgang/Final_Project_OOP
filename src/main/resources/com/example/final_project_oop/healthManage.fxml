                  <Pane prefHeight="130.0" prefWidth="200.0" style="-fx-border-color: #0093E0;">
                     <children>
                        <ImageView fitHeight="55.0" fitWidth="147.0" layoutX="25.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
